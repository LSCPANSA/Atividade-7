programa
{
	
	funcao inicio()
	{
		inteiro numeros[3][3] = 
		{
			{6, 1, 9},
			{4, 7, 5},
			{0, 3, 9}
		}

		inteiro soma = 0

		para(inteiro i = 0; i < 3; i++)
		{
			para(inteiro l = 0; l < 3; l++)
			{
			
			    soma = soma + numeros[i][l]
				
		    }
		}

		escreva("A soma de todos dÃ¡ ", soma, "!")
	}
}
